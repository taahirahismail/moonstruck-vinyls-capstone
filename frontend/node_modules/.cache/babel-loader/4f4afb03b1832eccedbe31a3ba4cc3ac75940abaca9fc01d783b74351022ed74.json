{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport SpinnerComp from \"@/components/SpinnerComp.vue\";\nexport default {\n  props: [\"product\"],\n  components: {\n    SpinnerComp\n  },\n  data() {\n    return {\n      search: \"\",\n      category: \"All\"\n    };\n  },\n  computed: {\n    products() {\n      return this.$store.state.products?.filter(product => {\n        let isMatch = true;\n        if (!product.albumName.toLowerCase().includes(this.search.toLowerCase())) {\n          isMatch = false;\n        }\n        if (this.genre !== \"All\" && this.genre !== product.genre) {\n          isMatch = false;\n        }\n        return isMatch;\n      });\n    }\n  },\n  mounted() {\n    this.$store.dispatch(\"fetchProducts\");\n  },\n  methods: {\n    viewProduct(prodID) {\n      const selectedProduct = this.products.find(product => product.prodID === prodID);\n      this.$store.commit(\"setSelectedProduct\", selectedProduct);\n      this.$router.push({\n        name: \"product\",\n        params: {\n          prodID: prodID\n        }\n      });\n    }\n\n    // sort by price\n\n    // sort by name\n  },\n\n  data() {\n    return {\n      search: \"\",\n      category: \"\"\n    };\n  }\n};","map":{"version":3,"names":["SpinnerComp","props","components","data","search","category","computed","products","$store","state","filter","product","isMatch","albumName","toLowerCase","includes","genre","mounted","dispatch","methods","viewProduct","prodID","selectedProduct","find","commit","$router","push","name","params"],"sources":["C:\\Users\\Student\\Desktop\\Development\\Capstone\\moonstruck-vinyls-capstone\\frontend\\src\\views\\ProductsView.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1 class=\"p-4 text-center heading-text\">Shop Our Vinyls !</h1>\r\n\r\n    <div>\r\n      <input type=\"text\" v-model=\"search\" placeholder=\"search\" />\r\n\r\n      <select v-model=\"genre\">\r\n        <option value=\"All\">All Genres</option>\r\n        <option value=\"Rock\">Rock</option>\r\n        <option value=\"Soul\">Soul</option>\r\n        <option value=\"Punk\">Punk</option>\r\n        <option value=\"Reggae\">Reggae</option>\r\n      </select>\r\n    </div>\r\n\r\n    <div v-if=\"products\" class=\"row m-3 d-flex justify-content-center\">\r\n      <div v-for=\"product in products\" :key=\"product\" :product=\"product\" class=\"col-6\">\r\n        <div class=\"card m-3 p-5\" id=\"prod-card\">\r\n          <div class=\"text-center prod-img\">\r\n            <img\r\n              :src=\"product.prodImg\"\r\n              :alt=\"product.albumName\"\r\n              class=\"img-fluid\"\r\n              id=\"image\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"text-center\">\r\n            <h2 class=\"heading-text\">{{ product.albumName }}</h2>\r\n            <cite class=\"gold-text\">{{ product.albumArtist }}</cite>\r\n\r\n            <div class=\"p-3 d-flex justify-content-between\">\r\n              R {{ product.price }}\r\n              <button\r\n                @click=\"viewProduct(product.prodID)\"\r\n                class=\"btn details-btn\"\r\n              >\r\n                View Details\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-else>\r\n      <spinner-comp class=\"mx-auto my-5\" />\r\n    </div>\r\n\r\n    <h3 class=\"text-center my-5 gold-text\">\r\n        More coming soon... !\r\n    </h3>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SpinnerComp from \"@/components/SpinnerComp.vue\";\r\n\r\nexport default {\r\n  props: [\"product\"],\r\n\r\n  components: {\r\n    SpinnerComp,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      search: \"\",\r\n      category: \"All\",\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    products() {\r\n      return this.$store.state.products?.filter((product) => {\r\n        let isMatch = true;\r\n        if (\r\n          !product.albumName.toLowerCase().includes(this.search.toLowerCase())\r\n        ) {\r\n          isMatch = false;\r\n        }\r\n        if (this.genre !== \"All\" && this.genre !== product.genre) {\r\n          isMatch = false;\r\n        }\r\n        return isMatch;\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.$store.dispatch(\"fetchProducts\");\r\n  },\r\n\r\n  methods: {\r\n    viewProduct(prodID) {\r\n      const selectedProduct = this.products.find(\r\n        (product) => product.prodID === prodID\r\n      );\r\n\r\n      this.$store.commit(\"setSelectedProduct\", selectedProduct);\r\n      this.$router.push({ name: \"product\", params: { prodID: prodID } });\r\n    },\r\n\r\n    // sort by price\r\n\r\n    // sort by name\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      search: \"\",\r\n      category: \"\",\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#prod-card {\r\n  background: #0a192f;\r\n  border: 3px solid #f4f4f4;\r\n  color: #f4f4f4;\r\n}\r\n\r\n.heading-text {\r\n    font-family: \"Archivo Black\", sans-serif;\r\n}\r\n\r\n.gold-text {\r\n  color: #ffd700;\r\n  font-family: 'Montserrat Alternates', sans-serif;\r\n}\r\n\r\n.details-btn {\r\n  background: #0a192f;\r\n  color: #ffd700;\r\n  box-shadow: 0 0 0 0 transparent;\r\n  -webkit-transition: all 0.2s ease-in;\r\n  -moz-transition: all 0.2s ease-in;\r\n  transition: all 0.2s ease-in;\r\n}\r\n\r\n.details-btn:hover {\r\n  box-shadow: 0 0 10px 1px #ffd700;\r\n  -webkit-transition: all 0.2s ease-out;\r\n  -moz-transition: all 0.2s ease-out;\r\n  transition: all 0.2s ease-out;\r\n}\r\n</style>\r\n"],"mappings":";AAyDA,OAAOA,WAAU,MAAO,8BAA8B;AAEtD,eAAe;EACbC,KAAK,EAAE,CAAC,SAAS,CAAC;EAElBC,UAAU,EAAE;IACVF;EACF,CAAC;EAEDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EAEDC,QAAQ,EAAE;IACRC,QAAQA,CAAA,EAAG;MACT,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,QAAQ,EAAEG,MAAM,CAAEC,OAAO,IAAK;QACrD,IAAIC,OAAM,GAAI,IAAI;QAClB,IACE,CAACD,OAAO,CAACE,SAAS,CAACC,WAAW,CAAC,